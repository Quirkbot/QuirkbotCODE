---
layout: null
---
{
	"version": "0.0.1",
	"nodes" : [
		{% for node in site.nodes %}
			{
				"id" : {{node.object_id | jsonify}},
				"index" : {{node.index | jsonify}},
				"color" : {{node.color | jsonify}},
				"type" : {{node.type | jsonify}},
				"taxonomy" : {{node.taxonomy | jsonify}},
				"inputs" : {{node.inputs | jsonify}},
				"outputs" : {{node.outputs | jsonify}}
			}{% if forloop.last == false %},{% endif %}
		{% endfor %}
	],
	"taxonomies": {
		"categories": [
			{% assign categories = (site.node_categories | sort: 'index') %}
			{% for category in categories %}
				{
					"id" : {{category.object_id | jsonify}},
					"index" : {{category.index | jsonify}},
					"color" : {{category.color | jsonify}}
				}{% if forloop.last == false %},{% endif %}
			{% endfor %}
		],
		"tags": [
			{
				"id" : "simple"
			},
			{
				"id" : "advanced"
			}
		]
	},
